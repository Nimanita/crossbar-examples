{
    "version": 2,
    "controller": {
        "id": "node1",
        "options": {
        }
    },
    "workers": [
        {
            "type": "router",
            "options": {
            },
            "realms": [
                {
                    "name": "realm1",
                    "roles": [
                        {
                            "name": "public",
                            "permissions": [
                                {
                                    "uri": "io.crossbar.demo.public.",
                                    "match": "prefix",
                                    "allow": {
                                        "call": true,
                                        "register": true,
                                        "publish": true,
                                        "subscribe": true
                                    },
                                    "disclose": {
                                        "caller": true,
                                        "publisher": true
                                    },
                                    "cache": true
                                }
                            ]
                        },
                        {
                            "name": "user",
                            "permissions": [
                                {
                                    "uri": "io.crossbar.demo.user.",
                                    "match": "prefix",
                                    "allow": {
                                        "call": true,
                                        "register": true,
                                        "publish": true,
                                        "subscribe": true
                                    },
                                    "disclose": {
                                        "caller": true,
                                        "publisher": true
                                    },
                                    "cache": true
                                }
                            ]
                        }
                    ]
                }
            ],
            "transports": [
                {
                    "type": "rawsocket",
                    "endpoint": {
                        "type": "unix",
                        "path": "router.sock"
                    },
                    "options": {
                        "max_message_size": 1048576
                    },
                    "serializers": ["cbor"],
                    "auth": {
                        "anonymous": {
                            "type": "static",
                            "role": "public"
                        },
                        "ticket": {
                            "type": "static",
                            "principals": {
                                "alice": {
                                    "ticket": "65daf2766173af06cc30",
                                    "role": "user"
                                }
                            }
                        }
                    }
                },
                {
                    "type": "websocket",
                    "endpoint": {
                        "type": "unix",
                        "path": "routerws.sock"
                    },
                    "serializers": ["json", "cbor"],
                    "auth": {
                        "anonymous": {
                            "type": "static",
                            "role": "public"
                        },
                        "ticket": {
                            "type": "static",
                            "principals": {
                                "alice": {
                                    "ticket": "65daf2766173af06cc30",
                                    "role": "user"
                                }
                            }
                        }
                    }
                }
            ]
        },

        {
            "type": "proxy",
            "options": {
            },
            "backends": [
                {
                    "realm": "foo",
                    "transport": {
                        "type": "rawsocket",
                        "endpoint": {
                            "type": "unix",
                            "path": "router.sock"
                        },
                        "url": "rs://localhost",
                        "serializer": "cbor"
                    }
                },
                {
                    "transport": {
                        "type": "websocket",
                        "endpoint": {
                            "type": "unix",
                            "path": "routerws.sock"
                        },
                        "url": "ws://localhost",
                        "serializers": ["json"]
                    }
                }
            ],
            "transports": [
                {
                    "type": "web",
                    "endpoint": {
                        "type": "tcp",
                        "port": 8443,
                        "shared": true,
                        "backlog": 1024
                    },
                    "paths": {
                        "/": {
                            "type": "static",
                            "directory": "../web",
                            "options": {
                                "enable_directory_listing": false
                            }
                        },
                        "autobahn": {
                            "type": "archive",
                            "archive": "autobahn.zip",
                            "origin": "https://github.com/crossbario/autobahn-js-browser/archive/master.zip",
                            "download": true,
                            "cache": true,
                            "mime_types": {
                                ".min.js": "text/javascript",
                                ".jgz": "text/javascript"
                            }
                        },
                        "ws": {
                            "type": "websocket",
                            "serializers": [
                                "cbor", "msgpack", "json"
                            ],
                            "options": {
                                "allowed_origins": [
                                    "https://demo.crossbar.io:*",
                                    "https://cbdemo-eu-central-1.crossbar.io:*",
                                    "https://cbdemo-us-west-1.crossbar.io:*",
                                    "https://cbdemo-ap-southeast-1.crossbar.io:*"
                                ],
                                "allow_null_origin": false,
                                "enable_webstatus": false,
                                "max_frame_size": 1048576,
                                "max_message_size": 1048576,
                                "auto_fragment_size": 65536,
                                "fail_by_drop": true,
                                "open_handshake_timeout": 2500,
                                "close_handshake_timeout": 1000,
                                "auto_ping_interval": 10000,
                                "auto_ping_timeout": 5000,
                                "auto_ping_size": 4,
                                "compression": {
                                    "deflate": {
                                        "request_no_context_takeover": false,
                                        "request_max_window_bits": 13,
                                        "no_context_takeover": false,
                                        "max_window_bits": 13,
                                        "memory_level": 5
                                    }
                                }
                            }
                        },
                        "info": {
                            "type": "nodeinfo"
                        }
                    }
                }
            ]
        },

        {
            "type": "proxy",
            "options": {
            },
            "backends": [
                {
                    "transport": {
                        "type": "rawsocket",
                        "endpoint": {
                            "type": "unix",
                            "path": "router.sock"
                        },
                        "url": "rs://localhost",
                        "serializer": "cbor"
                    }
                }
            ],
            "transports": [
                {
                    "type": "web",
                    "endpoint": {
                        "type": "tcp",
                        "port": 8443,
                        "shared": true,
                        "backlog": 1024
                    },
                    "paths": {
                        "/": {
                            "type": "static",
                            "directory": "../web",
                            "options": {
                                "enable_directory_listing": false
                            }
                        },
                        "autobahn": {
                            "type": "archive",
                            "archive": "autobahn.zip",
                            "origin": "https://github.com/crossbario/autobahn-js-browser/archive/master.zip",
                            "download": true,
                            "cache": true,
                            "mime_types": {
                                ".min.js": "text/javascript",
                                ".jgz": "text/javascript"
                            }
                        },
                        "ws": {
                            "type": "websocket",
                            "serializers": [
                                "cbor", "msgpack", "json"
                            ],
                            "options": {
                                "allowed_origins": [
                                    "https://demo.crossbar.io:*",
                                    "https://cbdemo-eu-central-1.crossbar.io:*",
                                    "https://cbdemo-us-west-1.crossbar.io:*",
                                    "https://cbdemo-ap-southeast-1.crossbar.io:*"
                                ],
                                "allow_null_origin": false,
                                "enable_webstatus": false,
                                "max_frame_size": 1048576,
                                "max_message_size": 1048576,
                                "auto_fragment_size": 65536,
                                "fail_by_drop": true,
                                "open_handshake_timeout": 2500,
                                "close_handshake_timeout": 1000,
                                "auto_ping_interval": 10000,
                                "auto_ping_timeout": 5000,
                                "auto_ping_size": 4,
                                "compression": {
                                    "deflate": {
                                        "request_no_context_takeover": false,
                                        "request_max_window_bits": 13,
                                        "no_context_takeover": false,
                                        "max_window_bits": 13,
                                        "memory_level": 5
                                    }
                                }
                            }
                        },
                        "info": {
                            "type": "nodeinfo"
                        }
                    }
                }
            ]
        }
    ]
}
